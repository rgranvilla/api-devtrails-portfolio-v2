{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "baseUrl": "./",
      "paths": {
        "@config/*": ["./src/config/*"],
        "@routes/*": ["./src/routes/*"],
      },
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true
    }
  }
  